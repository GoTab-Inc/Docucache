{
  "name": "@docucache/docucache",
  "version": "0.0.1",
  "module": "src/index.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./src/index.ts",
      "types": "./dist/.d.ts"
    }
  },
  "types": "./dist/.d.ts",
  "type": "module",
  "scripts": {
    "test": "bun test",
    "build": "rm -rf dist && bun build ./src/index.ts ./src/stores/*.ts --outdir ./dist --format esm --external '*' && bun run types",
    "types": "bunx tsc -p ./tsconfig.json"
  },
  "devDependencies": {
    "@types/bun": "^1.1.1"
  },
  "volta": {
    "node": "16.20.2"
  },
  "engines": {
    "node": ">=16"
  }
}