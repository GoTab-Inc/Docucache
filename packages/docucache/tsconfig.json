{
  "include": ["src/index.ts", "src/stores/*.ts"],
  "exclude": ["node_modules", "dist"],
  "compilerOptions": {
    "types": [],
    "declaration": true,
    "emitDeclarationOnly": true,
    "declarationMap": true,
    "outFile": "./dist/main.ts",
    "lib": ["ESNext"],
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "allowJs": true,
    "incremental": false
  },
}
